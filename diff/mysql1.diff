-
-    FixupFields(ctype);
-    ListSep ls("\n, ");
-
-    if (surrogate_pkey) {
-        createcmd.cmd << ls;
-        createcmd.cmd << " `"<<name_Get(ssimfile) << "_rowid` INT PRIMARY KEY AUTO_INCREMENT";
-    }
