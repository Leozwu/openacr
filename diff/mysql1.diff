-
-    while ((mysql_row = mysql_fetch_row(res.res)) != NULL) {
-        prlog_("*");
-        dmmeta::Field field;
-        field.field         = tempstr() << ctype.ctype<<"." << mysql_row[2];
-        field.dflt.value  = mysql_row[6];
-        ToBltin(mysql_row[3], field);
-        idx++;
-        prlog(field);
-    }
-}
